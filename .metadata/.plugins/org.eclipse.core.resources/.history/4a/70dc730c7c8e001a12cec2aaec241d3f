package codingbat;

public class Sum28 {
	public static boolean sum28(int[] nums) {
		int sum = 0;
		for (int i = 0; i < nums.length; i++) {
			if (nums[i] == 2) {
				sum = sum + nums[i];
			}
		}
		if (sum == 8) {
			return true;
		}
		return false;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		System.out.println(Sum28.sum28(new int[] { 2, 2, 5, 13, 8, 8, 7 }));

	}

}
