package codingbat;

public class Zeromax {

	public static int[] zeroMax(int[] nums) {

		int[] arr = new int[nums.length];
		for (int i = 0; i < nums.length; i++) {
			if (i < nums.length - 1 && nums[i] == 0 && nums[i + 1] / 2 == 1) {
				nums[i]=nums[i+1];
			}
		}
		return arr;
	}

	public static void main(String[] args) {

	}

}
